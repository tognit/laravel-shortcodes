{
    "name": "vedmant/laravel-shortcodes",
    "description": "Wordress like shortcodes for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Vedmant",
            "email": "vedmant@gmail.com",
            "homepage": "https://vedmant.com/laravel-shortcodes"
        }
    ],
    "homepage": "https://github.com/vedmant/laravel-shortcodes",
    "keywords": ["Laravel", "Shortcodes"],
    "require": {
        "php": "^7.0.0",
        "illuminate/contracts": "~5",
        "illuminate/view": "~5",
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "^3.8",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Vedmant\\LaravelShortcodes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vedmant\\LaravelShortcodes\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vedmant\\LaravelShortcodes\\LaravelShortcodesServiceProvider"
            ],
            "aliases": {
                "Shortcodes": "Vedmant\\LaravelShortcodes\\Facades\\Shortcodes"
            }
        }
    }
}
